<apex:page standardController="Opportunity" extensions="QuotePDF_CtrlExt" renderAs="pdf" sidebar="false" showHeader="false" applyHtmlTag="false">
     
    <head>
        <style type="text/css">
            @page{
            	margin-top: 140px;
            	margin-bottom: 140px;
            	margin-left: 0;
            	margin-right: 0;
            	padding-left: 0;
            	padding-right: 0;
            
                @top-center{
                    content: element(header);
                }
                @bottom-center{
                    content: element(footer);
                }
            }
            
            @page:first {
            	margin-top: -8px;
            	margin-bottom: 100px;
            
            	@top-center {
            		content: normal;
            	}
            
	            @bottom-center {
            		content: element(footer_first);
    	        }
            }
            
            body,
            div,
            p,
            tr,
            th,
            td {
